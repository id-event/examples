{
  "jti": "fb4e75b5411e4e19b6c0fe87950f7749",
  //event indicating further authentication occurred at the rp
  "events":[
    "https://openid.net/heart/parallel-authentication"
  ],
  //time of authentication event
  "iat": 1458496025,

  //this message is being sent from the rp to the idp
  "iss": "https://rp.example.com",
  "aud":[
    "https://idp.example.com"
  ],

  //parallel authentication would have four fields
  "https://openid.net/heart/parallel-authentication":{
    //context of the subject of the event
    "iss": "https://idp.example.com",
    //identifier of the subject of the event
    "sub": "248289761001",
    //boolean indicating whether authentication challenge was successful
    "authenticationSuccess": true,
    //authentication method
    "amr": "fpt"
  }
}